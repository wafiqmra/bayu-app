{
    "version": 2,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.2"
        }
    },
    "routes": [
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "env": {
        "APP_DEBUG": "true",
        "APP_ENV": "production",
        "LOG_CHANNEL": "stderr"
    }
}